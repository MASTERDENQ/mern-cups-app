{
  "name": "mernstack",
  "version": "1.0.0",
  "description": "mern stack app",
  "main": "index.js",
  "scripts": {
    "mern-install": "npm install && cd .. && cd client && npm install",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd .. && cd client && npm start",
    "mern": "concurrently \"npm run server\" \"npm run client\""
